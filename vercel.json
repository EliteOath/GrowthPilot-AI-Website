{
  "version": 2,
  "functions": {
    "api/handler.js": {
      "runtime": "nodejs20"
    }
  },
  "routes": [
    { "src": "/api/stripe-webhook", "dest": "/api/handler.js" },
    { "src": "/api/trpc(?:/.*)?", "dest": "/api/handler.js" },
    { "src": "/api/(.*)", "dest": "/api/handler.js" }
  ]
}
