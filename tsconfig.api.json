{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "api",
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": false
  },
  "include": ["api/trpc.ts"],
  "exclude": ["vite.config.ts", "api/server/_core/vite.ts"]
}